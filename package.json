{
  "name": "in2bonesv2",
  "version": "2.0.0",
  "description": "Planificateur 3D in2bones",
  "scripts": {
    "dev4": "SET NODE_ENV=dev&& webpack --mode=development --watch -d",
    "dev2": "gulp serve",
    "prod": "webpack --mode=production ",
    "dev": "concurrently --kill-others \"npm run dev4\" \"npm run dev2\"",
    "test": "npm run testPlanif && npm run testTibia && npm run testMortaiseTibia && npm run testImplantTibia && npm run testTalus && npm run testMortaiseTalus && npm run testImplantTalus && npm run testFibula",
    "testReport": "npm run testPlanifReport && npm run testTibiaReport && npm run testMortaiseTibiaReport && npm run testMortaiseTalusReport && npm run testImplantTalusReport && npm run testImplantTibiaReport",
    "testPlanifReport": "mocha test/test.preplanification.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testPlanification",
    "testTibiaReport": "mocha test/test.tibia.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testTibia",
    "testTalusReport": "mocha test/test.talus.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testTalus",
    "testMortaiseTibiaReport": "mocha test/test.mortaiseTibia.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testMortaiseTibia",
    "testMortaiseTalusReport": "mocha test/test.mortaiseTalus.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testMortaiseTalus",
    "testImplantTibiaReport": "mocha test/test.implantTibia.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testImplantTibia",
    "testImplantTalusReport": "mocha test/test.implantTalus.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testImplantTalus",
    "testFibulaReport": "mocha test/test.fibula.js --reporter mochawesome --reporter-options reportDir=test/test.doc,reportFilename=testFibula",
    "testPlanif": "mocha test/test.preplanification.js",
    "testTibia": "mocha test/test.tibia.js",
    "testTalus": "mocha test/test.talus.js",
    "testFibula": "mocha test/test.fibula.js",
    "testMortaiseTibia": "mocha test/test.mortaiseTibia.js",
    "testMortaiseTalus": "mocha test/test.mortaiseTalus.js",
    "testImplantTibia": "mocha test/test.implantTibia.js",
    "testImplantTalus": "mocha test/test.implantTalus.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "gulp": "^4.0.2",
    "jquery": "^3.3.1",
    "mochawesome": "^3.1.1",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.1.3",
    "webpack-livereload-plugin": "^2.1.1"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "melanke-watchjs": "^1.4.3",
    "mocha": "^5.2.0",
    "noty": "^3.2.0-beta",
    "rsvp": "^4.8.2",
    "sweetalert": "^2.1.0",
    "three": "^0.92.0",
    "three.meshline": "^1.4.0"
  }
}
